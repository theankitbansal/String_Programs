#include <iostream>

using namespace std;

int main()
{
    string str= "WeLcoMe";
    int i;
    for(i=0; str[i]!='\0'; i++){
    //Check if the character is upper case.
        if(str[i]>=65 && str[i]<=90){
            str[i]+=32;
        }
        //Check if the character is upper case.
        else if(str[i]>=97 && str[i]<=122){
            str[i]-=32;
        }
    }
    cout<<str;
}
