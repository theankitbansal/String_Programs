//Program to compare the string.

#include <iostream>

using namespace std;


int main(){
    string str1="Painter";
    string str2="Painting";
    int i,j;
    for(i=0, j=0; str1[i]!='\0' && str2[j]!='\0'; i++, j++){
        if(str1[i]!=str2[j]){
            break;
        }
    }
   if(str1[i]==str2[j])
   {
       cout<<"Equal";
   }else if(str1[i]<str2[j]){
       cout<<"String 1 is smaller";
   }else{
       cout<<"String 2 is smaller";
   }
    
}
